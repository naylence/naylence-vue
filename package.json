{
  "name": "@naylence/vue",
  "version": "0.1.0",
  "type": "module",
  "description": "Vue integration for Naylence Fame Fabric",
  "author": "Naylence Dev <naylencedev@gmail.com>",
  "license": "Apache-2.0",
  "keywords": [
    "naylence",
    "vue",
    "fame",
    "fabric",
    "composables"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/naylence/naylence-vue.git"
  },
  "bugs": {
    "url": "https://github.com/naylence/naylence-vue/issues"
  },
  "homepage": "https://github.com/naylence/naylence-vue#readme",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.vue",
    "lint:fix": "eslint src --ext .ts,.vue --fix",
    "format": "prettier --write \"src/**/*.{ts,vue}\"",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@naylence/agent-sdk": "^0.3.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^4.0.15",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^23.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "5.3.3",
    "vitest": "^4.0.15",
    "vue": "^3.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
